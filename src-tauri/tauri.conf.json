{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Ultra Gitlab",
  "version": "0.13.4",
  "identifier": "com.jens.ultra-gitlab",
  "build": {
    "beforeDevCommand": "bun run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "bun run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "createUpdaterArtifacts": true,
    "resources": {
      "../dist/*": "companion-dist/",
      "../dist/assets/*": "companion-dist/assets/"
    }
  },
  "plugins": {
    "deep-link": {
      "mobile": [],
      "desktop": {
        "schemes": ["ultra-gitlab"]
      }
    },
    "updater": {
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDZCODgyNTE2Mjc4QjM0NTMKUldSVE5Jc25GaVdJYS96aHBtL2J1dllLVUE1T1FCMHdjVUE1Tk1NTVhKVlBTc2E1QmF1bFQvdkMK",
      "endpoints": [
        "https://github.com/Jensderond/ultra-gitlab/releases/latest/download/latest.json"
      ]
    }
  }
}
